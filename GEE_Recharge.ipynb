        "    description=\"End year:\",\n",
        "    orientation=\"horizontal\",\n",
        "    readout=True,\n",
        "    readout_format=\"d\",\n",
        "    style=style\n",
        ")\n",
        "widget_zr = widgets.FloatText(\n",
        "    value=0.1,\n",
        "    description=\"Root depth (m):\",\n",
        "    step=0.01,\n",
        "    disabled=False,\n",
        "    style=style\n",
        ")\n",
        "widget_fr = widgets.FloatText(\n",
        "    value=1.0,\n",
        "    description=\"PET bias factor:\",\n",
